<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"

  xmlns:app="http://schemas.android.com/apk/res-auto"

  xmlns:tools="http://schemas.android.com/tools"

  android:layout_width="match_parent"

  android:layout_height="wrap_content"

  tools:context=".FilterFragment">


  <androidx.recyclerview.widget.RecyclerView
    android:id="@+id/First_Letter_Recycler_View"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:layout_marginStart="52dp"
    android:layout_marginBottom="8dp"
    app:layout_constraintBottom_toTopOf="@+id/filter_Clear_Age_Range_0_18_Button"
    app:layout_constraintStart_toEndOf="@+id/Filter_Clear_First_Letter_Clear_Button"
    tools:listitem="@layout/filter_fragment_item" />

  <Button
    android:id="@+id/Filter_Clear_First_Letter_Clear_Button"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:text="Clear"
    app:layout_constraintStart_toStartOf="parent"
    app:layout_constraintTop_toTopOf="parent" />

  <Button
    android:id="@+id/Filter_Clear_Age_Range_Button"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:layout_marginTop="67dp"
    android:text="Clear"
    app:layout_constraintStart_toStartOf="parent"
    app:layout_constraintTop_toTopOf="@+id/Filter_Clear_First_Letter_Clear_Button" />

  <Button
    android:id="@+id/filter_Clear_Age_Range_0_18_Button"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:layout_marginStart="24dp"
    android:layout_marginTop="67dp"
    android:text="0-18"
    android:textAlignment="textStart"
    android:textSize="5pt"
    app:layout_constraintStart_toEndOf="@+id/Filter_Clear_Age_Range_Button"
    app:layout_constraintTop_toTopOf="parent" />

  <Button
    android:id="@+id/filter_Clear_Age_Range_25_35_Button"
    android:layout_width="wrap_content"
    android:layout_height="0dp"
    android:layout_marginStart="8dp"
    android:layout_marginTop="67dp"
    android:text="25-35"
    android:textAlignment="textStart"
    android:textSize="5pt"
    app:layout_constraintStart_toEndOf="@+id/filter_Clear_Age_Range_18_25_Button"
    app:layout_constraintTop_toTopOf="parent" />

  <Button
    android:id="@+id/filter_Clear_Age_Range_35_55_Button"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:layout_marginStart="16dp"
    android:layout_marginTop="67dp"
    android:text="35-55"
    android:textAlignment="textStart"
    android:textSize="5pt"
    app:layout_constraintStart_toEndOf="@+id/filter_Clear_Age_Range_25_35_Button"
    app:layout_constraintTop_toTopOf="parent" />

  <Button
    android:id="@+id/filter_Clear_Age_Range_18_25_Button"
    android:layout_width="wrap_content"
    android:layout_height="0dp"
    android:layout_marginStart="16dp"
    android:layout_marginTop="67dp"
    android:text="18-25"
    android:textAlignment="textStart"
    android:textSize="5pt"
    app:layout_constraintStart_toEndOf="@+id/filter_Clear_Age_Range_0_18_Button"
    app:layout_constraintTop_toTopOf="parent" />

  <Button
    android:id="@+id/filter_Clear_Age_Range_55_older_Button"
    android:layout_width="wrap_content"
    android:layout_height="0dp"
    android:layout_marginStart="16dp"
    android:layout_marginTop="67dp"
    android:layout_marginEnd="16dp"
    android:text="55>"
    android:textAlignment="textStart"
    android:textSize="5pt"
    app:layout_constraintEnd_toEndOf="parent"
    app:layout_constraintStart_toEndOf="@+id/filter_Clear_Age_Range_35_55_Button"
    app:layout_constraintTop_toTopOf="parent" />

</androidx.constraintlayout.widget.ConstraintLayout>
